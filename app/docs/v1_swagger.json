    "definitions": {
        "models_v1.CreateBookingIn": {
            "type": "object",
            "required": [
                "booking_time",
                "booking_time_period",
                "booking_user_id",
                "date",
                "room_id",
                "schedule_id",
                "service_id",
                "store_id",
                "user_id"
            ],
            "properties": {
                "booking_time": {
                    "type": "string",
                    "example": "09:00"
                },
                "booking_time_period": {
                    "type": "integer",
                    "example": 60
                },
                "booking_user_id": {
                    "type": "integer",
                    "example": 1
                },
                "date": {
                    "type": "string",
                    "example": "2024-05-15"
                },
                "room_id": {
                    "type": "integer",
                    "example": 1
                },
                "schedule_id": {
                    "type": "integer",
                    "example": 1
                },
                "service_id": {
                    "type": "integer",
                    "example": 1
                },
                "store_id": {
                    "type": "integer",
                    "example": 1
                },
                "user_id": {
                    "type": "integer",
                    "example": 1
                }
            }
        },
        "models_v1.CreateServiceIn": {
            "type": "object",
            "required": [
                "description",
                "name",
                "timePeriod"
            ],
            "properties": {
                "description": {
                    "type": "string",
                    "format": "string"
                },
                "name": {
                    "type": "string",
                    "format": "string"
                },
                "timePeriod": {
                    "type": "integer",
                    "format": "integer",
                    "minimum": 1,
                    "example": 60
                }
            }
        },
        "models_v1.GetScheduleOut": {
            "type": "object",
            "properties": {
                "date": {
                    "type": "string",
                    "example": "2024-05-15"
                },
                "schedule_id": {
                    "description": "返回\"2024-05-15\"格式的 time.Time 字符串",
                    "type": "integer",
                    "example": 1
                },
                "stateful_time_slots": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models_v1.StatefulTimeSlot"
                    }
                },
                "time_period": {
                    "type": "integer",
                    "example": 60
                }
            }
        },
        "models_v1.LoginWeChatIn": {
            "type": "object",
            "required": [
                "code"
            ],
            "properties": {
                "code": {
                    "type": "string",
                    "format": "string"
                }
            }
        },
        "models_v1.QuickGenerateIn": {
            "type": "object",
            "required": [
                "daily_end_time",
                "daily_start_time",
                "generate_days",
                "service_id",
                "start_date",
                "user_id"
            ],
            "properties": {
                "daily_end_time": {
                    "type": "string",
                    "example": "21:00"
                },
                "daily_start_time": {
                    "type": "string",
                    "example": "09:00"
                },
                "generate_days": {
                    "type": "integer",
                    "example": 7
                },
                "service_id": {
                    "type": "integer",
                    "example": 1
                },
                "start_date": {
                    "type": "string",
                    "example": "2024-05-15"
                },
                "user_id": {
                    "type": "integer",
                    "example": 1
                }
            }
        },
        "models_v1.StatefulTimeSlot": {
            "type": "object",
            "properties": {
                "is_available": {
                    "type": "boolean",
                    "example": true
                },
                "time_slot": {
                    "type": "string",
                    "example": "09:00"
                }
            }
        }
    }
